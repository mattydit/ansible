global:
 scrape_interval: 10s
 evaluation_interval: 10s
scrape_configs:
 - job_name: 'kafka'
   static_configs:
    - targets:
      - Kafka-cp1:2021
      - Kafka-cp2:2021
      - Kafka-cp3:2021
 - job_name: 'zookeeper'
   static_configs:
    - targets:
      - Kafka-cp1:2020
      - Kafka2-cp2:2020
      - Kafka3-cp3:2020

